<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meetservice2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="meetservice.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="meetservice.NewRegActivity"
            android:label="@string/title_activity_new_reg" >
        </activity>
        <activity
            android:name="meetservice.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="meetservice.UserMain"
            android:label="@string/title_activity_user_main" >
        </activity>
        <activity
            android:name="meetservice.OfferService"
            android:label="@string/title_activity_offer_service" >
        </activity>
        <activity
            android:name="meetservice.NewServiceOffer"
            android:label="@string/title_activity_new_service_offer" >
        </activity>
        <activity
            android:name="meetservice.ServiceSelectedActivity"
            android:label="@string/title_activity_service_selected" >
        </activity>
        <activity
            android:name="meetservice.SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name="meetservice.SearchResultActivity"
            android:label="@string/title_activity_search_result"
            android:parentActivityName="meetservice.SearchActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.meetservice2.SearchActivity" />
        </activity>
        <activity
            android:name="meetservice.ServicePickedActivity"
            android:label="@string/title_activity_service_picked"
            android:parentActivityName="meetservice.SearchResultActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.meetservice2.SearchResultActivity" />
        </activity>
        <activity
            android:name="meetservice.RecomendacionCalificacionActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_recomendacion_calificacion"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name="meetservice.AdminServiceActivity"
            android:label="@string/title_activity_admin_service" >
        </activity>
        <activity
            android:name="meetservice.CurrentServicesActivity"
            android:label="@string/title_activity_last_list_service" >
        </activity>
        <activity
            android:name="meetservice.ServiceOption"
            android:label="@string/title_activity_service_option" >
        </activity>
        <activity
            android:name="meetservice.TakeServiceActivity"
            android:label="@string/title_activity_take_service" >
        </activity>
    </application>

</manifest>