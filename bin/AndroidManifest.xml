<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meetservice2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.meetservice2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.meetservice2.NewRegActivity"
            android:label="@string/title_activity_new_reg" >
        </activity>
        <activity
            android:name="com.example.meetservice2.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.meetservice2.UserMain"
            android:label="@string/title_activity_user_main" >
        </activity>
        <activity
            android:name="com.example.meetservice2.OfferService"
            android:label="@string/title_activity_offer_service" >
        </activity>
        <activity
            android:name="com.example.meetservice2.NewServiceOffer"
            android:label="@string/title_activity_new_service_offer" >
        </activity>
    </application>

</manifest>